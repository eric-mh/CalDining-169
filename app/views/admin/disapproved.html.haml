.container
  .navbar.clear
    = render 'shared/navbar'
  .header Disapproved Images
.container-big.clear
  - if @disapproved_images.empty? 
    %div No disapproved images available.
  - else
    - @disapproved_images.each do |image|
      .item-container
        .item-image-container
          %img{:src => image.filename, :class => 'img_scaled_wh'}
          = button_to 'Approve', image_approve_path(image.id)
        .item-text-container
          = image.item.name

